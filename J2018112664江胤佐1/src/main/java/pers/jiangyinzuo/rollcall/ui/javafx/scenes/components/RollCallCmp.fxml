<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="58.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pers.jiangyinzuo.rollcall.ui.javafx.controller.components.RollCallCmpController">
   <children>
      <Text fx:id="studentNameText" layoutX="35.0" layoutY="34.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <Text fx:id="studentIdText" layoutX="129.0" layoutY="34.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <MenuButton fx:id="presence" layoutX="450.0" layoutY="18.0" mnemonicParsing="false" text="已到">
        <items>
          <MenuItem fx:id="item1" mnemonicParsing="false" onAction="#changePresence" text="未到" />
          <MenuItem fx:id="item2" mnemonicParsing="false" onAction="#changePresence" text="迟到" />
            <MenuItem fx:id="item3" mnemonicParsing="false" onAction="#changePresence" text="请假" />
            <MenuItem fx:id="item4" mnemonicParsing="false" onAction="#changePresence" text="早退" />
        </items>
      </MenuButton>
      <Button fx:id="submitBtn" layoutX="536.0" layoutY="17.0" mnemonicParsing="false" onAction="#submit" text="提交" />
   </children>
</AnchorPane>
